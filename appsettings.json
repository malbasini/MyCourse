{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db"
  },
  "Courses": {
    "PerPage": 10,
    "InHome": 3,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  },
  "ResponseCache": {
    "Home": {
      "Duration": 60,
      "Location": "Client",
      "VaryByQueryKeys": ["page"]
    }
  },
  "IMemoryCache": {
    "FromSecond": 60
  }
}